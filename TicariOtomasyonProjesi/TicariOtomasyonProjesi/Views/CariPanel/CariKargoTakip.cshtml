@using TicariOtomasyonProjesi.Models.Siniflar
@model List<KargoTakip>

@{
    ViewBag.Title = "CariKargoTakip";
    Layout = "~/Views/Shared/_Carilayout.cshtml";
}



<br />
<table class="table table-striped">
    <tr>
        <th> Açıklama</th>
        <th>Tarih</th>

    </tr>

    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Aciklama</td>
            <td>@x.TarihZaman</td>


        </tr>
    }
</table>